{
  "name": "10mg",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && mv dist/10mg _static",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "deploy": "export NODE_OPTIONS=--openssl-legacy-provider && ng build && aws s3 sync dist/10mg s3://10mg.co.uk --exclude --profile=10mg && aws cloudfront create-invalidation --distribution-id E2D3Y3YZEZNAQJ  --paths '/*' --profile=10mg"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/fire": "^7.5.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "angular2-chartjs": "^0.5.1",
    "chart.js": "^4.2.0",
    "firebase": "^9.15.0",
    "rxjs": "~7.5.0",
    "swiper": "^8.4.5",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.9",
    "@angular/cli": "~14.2.9",
    "@angular/compiler-cli": "^14.2.0",
    "@types/jasmine": "~4.0.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.7.2"
  }
